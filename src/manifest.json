{
    "manifest_version": 2,
    "name": "le Dashboard",
    "version": "0.2.1",

    "description": "Replace new tab page with your long term goals.",
    "icons": {
        "128": "tab-icon.png"
    },

    "author": "Thomas Petracco",

    "browser_action": {
        "default_icon": "tab-icon.png"
    },

    "chrome_url_overrides" : {
        "newtab": "index.html"
    },

    "background" : {
        "scripts": [ "background.js" ]
    },

    "permissions": [
        "activeTab",
        "storage",
        "https://api.ledashboard.com/"
    ]
}
