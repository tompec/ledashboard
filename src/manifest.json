{
    "manifest_version": 2,
    "name": "LeDashboard",
    "version": "0.1.3",

    "description": "Keep track of your long-term goals.",
    "icons": {
        "128": "tab-icon.png"
    },

    "author": "Thomas Petracco",

    "browser_action": {
        "default_icon": "tab-icon.png"
    },

    "chrome_url_overrides" : {
        "newtab": "index.html"
    },

    "background" : {
        "scripts": [ "background.js" ]
    },

    "permissions": [
        "activeTab",
        "storage"
    ]
}
